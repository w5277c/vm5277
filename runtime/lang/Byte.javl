import lang.Number;

public class Byte implements Number {
  public	final	static	byte	MIN_VALUE	= 0x00;
  public	final	static	byte	MAX_VALUE	= 0xff;
  private	byte	value;

  public Byte(byte value) {
	this.value = value;
  }

  public bool toBool() {
	return 0x00!=value;
  }
  public byte toByte() {
	return value;
  }
  public short toShort() {
	return value;
  }
  public int toInt() {
	return value;
  }
  public fixed toFixed() {
	return (fixed)vaule;
  }
}